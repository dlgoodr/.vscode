[
  {
    "key": "ctrl+l",
    "command": "extension.prettyJSON",
    "when": "editorLangId == json"
  },
  {
    "key": "ctrl+l",
    "command": "extension.prettyJSON",
    "when": "editorLangId == jsonc"
  },
  {
    "key": "ctr+alt+m",
    "command": "-extension.prettyJSON"
  },
  {
    "key": "ctrl+l",
    "command": "python.runLinting",
    "when": "editorLangId == python"
  },
  {
    "key": "ctrl+alt+s",
    "command": "editor.action.sortLinesAscending"
  }
]
